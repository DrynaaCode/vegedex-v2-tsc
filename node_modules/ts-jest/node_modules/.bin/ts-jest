#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/Randy/Desktop/Projet Draveil/API/api-vegedex-2/node_modules/.pnpm/ts-jest@29.4.0_@babel+core@_ff4d3ecda98715a399e813b02d97f790/node_modules/ts-jest/node_modules:/mnt/c/Users/Randy/Desktop/Projet Draveil/API/api-vegedex-2/node_modules/.pnpm/ts-jest@29.4.0_@babel+core@_ff4d3ecda98715a399e813b02d97f790/node_modules:/mnt/c/Users/Randy/Desktop/Projet Draveil/API/api-vegedex-2/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/Randy/Desktop/Projet Draveil/API/api-vegedex-2/node_modules/.pnpm/ts-jest@29.4.0_@babel+core@_ff4d3ecda98715a399e813b02d97f790/node_modules/ts-jest/node_modules:/mnt/c/Users/Randy/Desktop/Projet Draveil/API/api-vegedex-2/node_modules/.pnpm/ts-jest@29.4.0_@babel+core@_ff4d3ecda98715a399e813b02d97f790/node_modules:/mnt/c/Users/Randy/Desktop/Projet Draveil/API/api-vegedex-2/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../cli.js" "$@"
else
  exec node  "$basedir/../../cli.js" "$@"
fi
